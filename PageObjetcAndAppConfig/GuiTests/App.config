<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- 
      Configure Selenium grid settings http://www.seleniumhq.org/projects/grid/
    -->
    <add key="UseGrid" value="false"/>
    <add key="GridUrl" value="http://riskitseleniumgrid:4444/wd/hub"/>
    <!--
      Choose on of the following drivers:
      InternetExplorer
      Chrome
      Firefox
    -->
    <!--<add key="DriverToUse" value="InternetExplorer" />
    <add key="DriverToUse" value="Firefox" />-->
    <add key="DriverToUse" value="Chrome"/>
    <add key="TargetUrl" value="http://demo.testfire.net/"/>
    <add key="ImplicitlyWait" value="10"/>
    <add key="PageLoadTimeout" value="15"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
